    <script type="text/javascript" src="https://www.google.com/jsapi?autoload={'modules':[{'name':'visualization','version':'1','packages':['corechart'], 'language': 'nl'}]}"></script>
    <script type="text/javascript">

        function refreshChart() {
            // Re-draw the chart.
            $('#chart-property-value').html('');
            drawVisualization();
        }

        var refreshCartCallback;
        $(function () {
            $(window).resize(function () {
                if (refreshCartCallback) {
                    clearTimeout(refreshCartCallback);
                }
                refreshCartCallback = setTimeout(refreshChart, 1000);
            });
        });


        var parseJsonDate = function (date) {
            /// <summary>
            /// Parses a date string in the JSON format ('Date([timestamp])') to a JavaScript date object.
            /// </summary>
            /// <param name="date" type="String">The date string in JSON format.</param>
            return new Date(parseInt(date.replace("/Date(", "").replace(")/", ""), 10));
        };

        var drawVisualization = function () {
            if ('False' === 'True') {
                if (nl.jaap.pageData) {
                    var data = new google.visualization.DataTable();
                    data.addColumn('date', 'Datum');
                    data.addColumn('number', 'Prijs');

                    var prev = null;
                    var minprice = 999999999;
                    var maxprice = 0;

                    var piceHistory = $.map(nl.jaap.pageData.priceHistory, function (item) {
                        var res;
                        if (prev === null) {
                            // The first item.
                            res = [[parseJsonDate(item.date), item.price]];
                        }
                        else {
                            // We want a rectangular chart.
                            // First add the price of the previous item with the date of the current item to
                            // create a straight line to the current date. Then add the current item's price and date
                            // to create a line straight up/down.
                            res = [[parseJsonDate(item.date), prev.price], [parseJsonDate(item.date), item.price]];
                        }

                        // Save the current item as previous item.
                        prev = item;

                        if (item.price > maxprice) {
                            maxprice = item.price;
                        }
                        if (item.price < minprice) {
                            minprice = item.price;
                        }
                        return res;
                    });

                    data.addRows(piceHistory);

                    // Determine the min and max value for the chart.
                    minprice = minprice - maxprice * 0.1;
                    maxprice = maxprice + maxprice * 0.1;

                    // Create a formatter for currency.
                    var formatter = new google.visualization.NumberFormat({
                        prefix: '€ ',
                        decimalSymbol: ',',
                        groupingSymbol: '.',
                        fractionDigits: 0
                    });

                    // Format the data in column with index 1 (price).
                    formatter.format(data, /* column index */ 1);

                    var lineChart = new google.visualization.LineChart(document.getElementById('chart-property-value'));
                    lineChart.draw(data, {
                        hAxis: { format: 'MMM yy' },
                        vAxis: { format: '€ ###,###' },
                        legend: { position: 'none' },
                        fontName: 'Open Sans',
                        max: maxprice, // Override the automatic default
                        min: minprice // Override the automatic default
                    });
                }
            }
        };

        google.setOnLoadCallback(drawVisualization);
    </script>
    <script type="text/javascript" src="https://www.google.com/jsapi?autoload={'modules':[{'name':'visualization','version':'1','packages':['corechart','table']}]}"></script>
    <script type="text/javascript">
        function isScrolledIntoView(elem) {
            var $elem = $(elem);
            var $window = $(window);

            var docViewTop = $window.scrollTop();
            var docViewBottom = docViewTop + $window.height();

            var elemTop = $elem.offset().top;
            var elemBottom = elemTop + $elem.height();

            return ((elemBottom >= docViewTop) && (elemTop <= docViewBottom));
        }

        function loadSpotziImages() {
            var images = $('img[data-src]');
            for (var i = 0; i < images.length; i++) {
                if (isScrolledIntoView(images[i]) && typeof $(images[i]).attr("src") == "undefined") {
                    $(images[i]).attr("src", $(images[i]).attr("data-src"));
                }
            }
        }

        $(document).ready(function() {
            $(window).scroll(function() {
                loadSpotziImages();
            });
        });

        var drawCharts = function () {
            if (nl.jaap.pageData) {
                if (nl.jaap.pageData.chartData && nl.jaap.pageData.chartData.length > 0) {
                    for (var i = 0; i < nl.jaap.pageData.chartData.length; i++) {
                        var mydata = nl.jaap.pageData.chartData[i];
                        var data = new google.visualization.DataTable();
                        data.addColumn('string', 'Label');
                        data.addColumn('number', 'Percentage');
                        var arr = $.map(mydata.data, function (value, key) { return [[key, value]]; });
                        data.addRows(arr);
                        var options = {
                            title: mydata.title,
                            "backgroundColor": 'transparent',
                            "width": 300,
                            "height": 160,
                            "chartArea": {
                                "width": '100%'
                            },
                            slices: { 0: { color: '#DF2617' }, 1: { color: '#0088CC' }, 2: { color: '#3F3F3F' }, 3: { color: '#CCCCCC' }, 4: { color: '#37A800' } },
                            legend: { position: 'right', textStyle: { color: '#333333', fontSize: 10 }, alignment: 'start' },
                        };
                        var chart = new google.visualization.PieChart(document.getElementById(mydata.id));
                        chart.draw(data, options);

                    }
                }
            }
        }

        google.setOnLoadCallback(drawCharts);
        
        setTimeout(function() {
            loadSpotziImages();
        }, 5000);
    </script>

    
